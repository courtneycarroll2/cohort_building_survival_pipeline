{
    "name": "LUAD_TP53_mutation_ns_T2",
    "description": "TCGA LUAD patients with TP53 mutations, T stage 2/2a/2b, and no history of smoking",
    "cancer_type": "LUAD",

    "clinical_criteria": [
        {
            "field": "tobacco_smoking_history",
            "logic": "equals",
            "value": 1
        },

        {
            "field": "pathologic_T",
            "logic": "in",
            "value": ["T2", "T2a", "T2b"]
        }

    ],

    "genomic_criteria": [
        {
            "field": "TP53",
            "logic": "equals",
            "value": 1
        }
    ],

    "model":
    {
        "time_start": "days_to_initial_pathologic_diagnosis",
        "time_end_event": "days_to_death",
        "time_end_censor": "days_to_last_followup",
        "covars": [
            {
                "field": "age_at_initial_pathologic_diagnosis",
                "type": "numeric",
                "ds": "clinical"
            } ,

            {
                "field": "gender",
                "type": "categorical",
                "ds": "clinical"
            },

            {
                "field": "pathologic_M",
                "type": "categorical",
                "ds": "clinical",
                "exclude": ["MX"],
                "binarize": 
                {
                    "zero": ["M0"],
                    "one": ["M1", "M1a", "M1b"]
                }
            },

            {
                "field": "pathologic_N",
                "type": "categorical",
                "ds": "clinical",
                "exclude": ["NX"],
                "binarize": 
                {
                    "zero": ["N0"],
                    "one": ["N1", "N2", "N3"]
                }
            }
        ]
    },

    "qc": {
        "proportion_missing_rm_variable": 0.2
    }

}